<?xml version="1.0" encoding="UTF-8"?>
<Integrador>
  <Identificador>
    <Valor>{{ numero_identificador }}</Valor>
  </Identificador>
  <Componente Nome="NFCE">
    <Metodo Nome={consulta.metodo}>
<Parametros>
<Parametro>
<Nome>numeroSessao</Nome>
<Valor>{{ consulta.numero_sessao }}</Valor>
</Parametro>
<Parametro>
<Nome>versaoDados</Nome>
<Valor>4.0</Valor>
</Parametro>
<Parametro>
<Nome>cUF</Nome>
<Valor>23</Valor>
</Parametro>
<Parametro>
<!--string base64 Contendo o XML da NFCE gerada pelo sistema-->
<Nome>dados</Nome>
<Valor><![CDATA[{{ consulta.cfe_venda }}]]></Valor>
</Parametro>
<Parametro>
<Nome>NumeroNFCe</Nome>
<!--Deve ser o mesmo número que está no base 64	-->
<Valor>{consulta.chave}</Valor>
</Parametro>
<Parametro>
<!--Deve ser a mesma data e hora que está no base 64-->
<Nome>DataHoraNFCeGerado</Nome>
<Valor>{consulta.data_hora}</Valor>
</Parametro>
<Parametro>
<!--Deve ser o mesmo valor que está no base 64-->
<Nome>ValorNFCe</Nome>
<Valor>{consulta.valor}</Valor>
</Parametro>
</Parametros>
</Metodo>
</Componente>
</Integrador>
